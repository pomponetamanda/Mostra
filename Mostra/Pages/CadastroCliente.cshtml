@page
@model Mostra.Pages.CadastroClienteModel

<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Império Feminino</title>
    <link rel="stylesheet" href="/css/cadastroempresa.css" />
</head>
<body>

        <h2>Cadastro de Cliente</h2>
    <form method="post">
        <label for="nome">Marca:</label>
        <input type="text" name="nome" required /><br />

        <label for="email">Modelo:</label>
        <input type="text" name="email" required /><br />

        <label for="cpf">Ano:</label>
        <input type="number" name="cpf" required /><br />

        <label for="telefone">Preço:</label>
        <input type="number" name="telefone" required /><br />

        <label for="endereco">Estoque:</label>
        <input type="number" name="endereco" required /><br />

        <label for="senha">Estoque:</label>
        <input type="number" name="senha" required /><br />

        <button type="submit">Adicionar</button>
    </form>

</body>

</html> 